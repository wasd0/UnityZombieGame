+            Func<InjectContext, IEnumerable<object>> method,
+            DiContainer container)
+        {
+            _container = container;
+            _method = method;
+        }
+
+        public bool IsCached
+        {
+            get { return false; }
+        }
+
+        public bool TypeVariesBasedOnMemberType
+        {
+            get { return false; }
