+        public static IProvider CreateCachedProvider(IProvider creator)
+        {
+            if (creator.TypeVariesBasedOnMemberType)
+            {
+                return new CachedOpenTypeProvider(creator);
+            }
+
+            return new CachedProvider(creator);
+        }
