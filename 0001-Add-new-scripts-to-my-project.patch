+    public PlayerEntity Create()
+    {
+        Vector3 spawnPosition = _spawnPosition.Point.transform.position;
+        
+        return _diContainer.
+            InstantiatePrefabResourceForComponent<PlayerEntity>(
+                PLAYER_PATH, spawnPosition,
+                Quaternion.identity, null);
+    }
+}
