+        {
+            var that = obj as ValuePair<T1, T2>;
+
+            if (that == null)
+            {
+                return false;
+            }
+
+            return Equals(that);
+        }
+
+        public bool Equals(ValuePair<T1, T2> that)
+        {
+            if (that == null)
+            {
+                return false;
+            }
+
