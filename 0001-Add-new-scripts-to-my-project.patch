+            if (!ZenUtilInternal.IsNull(instance))
+            {
+                Assert.That(instance.GetType().DerivesFromOrEqual(baseType),
+                    "Invalid type given during bind command.  Expected type '{0}' to derive from type '{1}'", instance.GetType(), baseType);
+            }
+        }
